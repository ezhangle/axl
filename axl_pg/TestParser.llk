LL (3);

unary_expr
	: unary_operator* postfix_expr
	;

unary_operator
	: '-'
	| '~'
	| resolver (cast_operator_rslv)
	  '(' EToken_Identifier ')'
	;

cast_operator_rslv
	: '(' EToken_Identifier ')'
	;

postfix_expr
	: primary_expr
	;

primary_expr
	: EToken_Identifier
	| EToken_Integer
	| '(' unary_expr ')'
	;

