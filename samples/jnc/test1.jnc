class CMyScheduler: jnc.IScheduler
{
	override
	Schedule (function* pf ())
	{
		printf ("before call\n");
		pf ();
		printf ("after call\n");
	} 
}

int 
main ()
{
	printf ("main ()\n");

	CMyScheduler Scheduler;
	(foo @ Scheduler) (100);

	return 10; 
}

foo (int x)
{
	printf (0x"61 62 63 " "_" $"foo ($x)" 0x" 20 64 65 66" "\n");
}

