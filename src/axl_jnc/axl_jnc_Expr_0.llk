			}
	|	'*' unary_expr_0 <$pValue> 
			{
				return m_pModule->m_OperatorMgr.GetUnaryOperatorResultType (EUnOp_Indir, $pValue);
			}
	|	'!' unary_expr_0 <$pValue> 
			{
				return m_pModule->m_OperatorMgr.GetUnaryOperatorResultType (EUnOp_LogNot, $pValue);
			}
	|	EToken_Inc unary_expr_0 <$pValue> 
