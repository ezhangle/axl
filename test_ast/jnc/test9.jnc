class CBase
{
	preconstruct ()
	{
		printf ("CBase.preconstruct ()\n");
		Foo ();
	}

	this ()
	{
		printf ("CBase.this ()\n");
		Foo ();
	} 

	this (int x)
	{
		printf ("CBase.this (%d)\n", x);
		Foo ();
	}

	~this ()
	{
		printf ("CBase.~this ()\n");
		Foo ();
	}

	virtual
	Foo ()
	{
		printf ("CBase.Foo ()\n");
	}
};

//. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

class CTest: CBase
{
	preconstruct ()
	{
		printf ("CTest.preconstruct ()\n");
		Foo ();
	}

	this ()
	{
		printf ("CTest.this ()\n");
		Foo ();
	}

	this (int x)
	{
		CBase (x * 2);
		printf ("CTest.this (%d)\n", x);
		Foo ();
	}

	~this ()
	{
		printf ("CTest.~this ()\n");
		Foo ();
	}

	override
	Foo ()
	{
		printf ("CTest.Foo ()\n");
	}
};

