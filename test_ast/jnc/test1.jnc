class IFoo
{
	int m_FooValue;

	abstract
	int 
	Foo (int x);
};

class IBar
{
	int m_BarValue;

	abstract
	int 
	Bar (int x);
};

class CTest: IFoo, IBar
{
	int m_TestValue;

	preconstruct ()
	{
		m_FooValue = 100;
		m_BarValue = 200;
		m_TestValue = 300;
	}	 

	override 
	int 
	Foo (int x)
	{
		return printf (
			"CTest.Foo (%d) { %d, %d, %d } \n", 
			x,
			m_FooValue,
			m_BarValue,
			m_TestValue
			);
	}

	override 
	int 
	Bar (int x)
	{
		return printf (
			"CTest.Bar (%d) { %d, %d, %d } \n", 
			x,
			m_FooValue,
			m_BarValue,
			m_TestValue
			);
	}
};


int
main ()
{
	printf ("hui govno i muravei\n");

	CTest t = new CTest;
	IFoo f = t;
	IBar b = t;

	int function* pfn () = b.Bar ~(5);
	pfn ();

	return 0;
}

