//.............................................................................

bindable int Red;
bindable int Green;
bindable int Blue;

autoev AutoEv
{
	printf ("Red = %02x\n", Red);
	printf ("Green = %02x\n", Green);
	printf ("Blue = %02x\n", Blue);

	printf ("RGB (%02x %02x, %02x)\n", Red, Green, Blue);
};

//.............................................................................

int
main ()
{
	stack new AutoEv;

	Red = 0xab;
	Green = 0xcd;
	Blue = 0xef;

	printf ("done\n");
	return 0;
}

