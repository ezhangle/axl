class IFoo
{
	protected int m_FooValue;

	readonly int m_BarValue;

	abstract 
	int 
	property FooProp;
};

class CTest: IFoo
{
	int m_TestValue;

	preconstruct ()
	{
		m_BarValue = 10;
		m_FooValue = 100;
		m_TestValue = 300;
	}	 

	override 
	int 
	FooProp.get ()
	{
		return printf (
			"CTest.FooProp.get () { %d, %d } \n", 
			m_FooValue,
			m_TestValue
			);
	}

	override 
	FooProp.set (int x)
	{
		printf (
			"CTest.FooProp.set (%d) { %d, %d } \n", 
			x,
			m_FooValue,
			m_TestValue
			);
	}
};

int 
main ()
{
	printf ("hui govno i muravei\n");

	IFoo f = new CTest;
	f.m_FooValue = 100;

	int y = f.FooProp;
	f.FooProp = 500;	

	return 0;
}

