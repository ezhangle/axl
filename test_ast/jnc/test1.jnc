struct TPoint
{
	int m_x = 5;

	int m_y;
	int m_z;
}

int 
main ()
{
	printf ("bindable property test\n");


	int x;

	TPoint p;

	p = { , 20, m_z = 30 };

	printf ("p = { %d, %d, %d }\n", p.m_x, p.m_y, p.m_z);

	TPoint* p2 = stack new TPoint { , 6, m_z = 7 };
	printf ("p2 = { %d, %d, %d }\n", p2.m_x, p2.m_y, p2.m_z);

	return 0;
}

