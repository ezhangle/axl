interface IFoo
{
	int m_a;
	int m_b;

	int property FooProp;

	int
	FooTest (
		int x,
		int y
		);
};

class CTest: IFoo
{
	int m_c;
	int m_d;	

	int
	FooTest (
		int x,
		int y
		)
	{
		printf ("CTest { %d; %d; %d; %d; }.FooTest (%d, %d);\n", m_a, m_b, m_c, m_d, x, y);
		return 0;
	}

	int 
	FooProp.get ()
	{
		printf ("CTest.FooProp::get ()\n");
		return m_a;
	}	

	void
	FooProp.set (int x)
	{
		printf ("CTest.FooProp::set (%d)\n", x);
		m_a = x;
	} 
};

