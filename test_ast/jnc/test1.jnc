class CTest
{
public:
	mutable int m_Value;

	Foo (int x) const
	{
		printf ("CTest.Foo (int x = %d)\n", x);
		m_Value = x;
	}

	Bar ()
	{
		printf ("CTest.Bar () { m_v = %d }\n", m_Value);
	}

	int
	operator ~ ()
	{
		printf ("CTest.operator ~ () { m_v = %d }\n", m_Value);
		return 123;
	}

	int 
	operator - ()
	{
		printf ("CTest.operator - () { m_v = %d }\n", m_Value);
		return 456;
	}

	int 
	operator - (int x)
	{
		printf ("CTest.operator - (int x = %d) { m_v = %d }\n", x, m_Value);
		return m_Value - x;
	}

};


