int g_Baud;
int g_Baud2;

bindable property 
{
	int
	get ()
	{
		printf ("PBaud.get () = %d\n", g_Baud);
		return g_Baud;
	}

	set (int Value)
	{
		printf ("PBaud.set (%d)\n", Value);
		g_Baud = Value;
	}
} PBaud;

//.............................................................................

OnBaudChange ()
{
	printf ("OnBaudChange (): Baud has changed to %d!\n", PBaud);
}

autoev ASerial
{
	printf ("autoev ASerial: Baud has changed to %d!\n", PBaud);
};
 
int
main ()
{
	printf ("hui\n");
	
	onchange (PBaud) += OnBaudChange;
	PBaud = 100;

	printf ("i muravei\n");
	return 0;
}

