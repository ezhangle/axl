//. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

property MyProp
{
	int propvalue;
	event onchange ();
		
	set (int x)
	{
		printf ("MyProp.set (%d)\n", x);

		propvalue = x;		

		if (x > 10)
			silent return;
	}
}

//. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

foo ()
{
	printf ("foo ()\n");
}

int 
main ()
{
	printf ("bindable property test\n");

	MyProp.onchange += foo;

	MyProp = 10;
	MyProp = 20;

	int x = MyProp;
	printf ("x = %d\n", x);

	return 0;
}

