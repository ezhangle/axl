struct TPoint
{
	int m_x;
	int m_y;

	Setup (
		int x,
		int y
		)
	{
		printf ("TPoint.Setup (%d, %d)\n", x, y);	

		m_x = x;
		m_y = y;
	}
};

//. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

int 
main ()
{
	printf ("struct / union members\n");

	TPoint p;
	p.m_x = 1;
	p.m_y = 2;

	p.Setup (10, 20);


	printf ("{ %d, %d }\n", p.m_x, p.m_y);

	return 0; 
}
 
