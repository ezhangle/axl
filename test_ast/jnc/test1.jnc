class CMyClass
{
	int m_x = 10;
	int m_y = foo (m_x);

	preconstruct ()
	{
		printf ("CMyClass.preconstruct () { m_x = %d; m_y = %d }\n", m_x, m_y);
	}	

	this ()
	{
		printf ("CMyClass.this () { m_x = %d; m_y = %d }\n", m_x, m_y);
	}

	int
	foo (int x)
	{
		return x * 20;
	}
};

//. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

int 
main ()
{
	printf ("field initializers test\n");

	heap new CMyClass;

	return 0;
}
 
