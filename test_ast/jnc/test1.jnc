class IFoo
{
	readonly int m_FooValue;

	abstract 
	int property FooProp;
};

class CTest: IFoo
{
	int m_TestValue;

	preconstruct ()
	{
		m_FooValue = 100;
		m_TestValue = 300;
	}	 

	override 
	int 
	FooProp.get ()
	{
		return printf (
			"CTest.FooProp.get () { %d, %d } \n", 
			m_FooValue,
			m_TestValue
			);
	}

	override 
	FooProp.set (int x)
	{
		printf (
			"CTest.FooProp.set (%d) { %d, %d } \n", 
			x,
			m_FooValue,
			m_TestValue
			);
	}
};

int
main ()
{
	printf ("hui govno i muravei\n");

	IFoo f = new CTest;

	int x = f.m_FooValue;
	
	int y = f.FooProp;
	f.FooProp = 500;	

	return 0;
}

