class IFoo
{
	int m_FooValue;

	abstract
	int 
	Foo ();
};

class IBar
{
	int m_BarValue;

	abstract
	int 
	Bar ();
};

class CTest: IFoo, IBar
{
	int m_TestValue;

	preconstruct ()
	{
		m_FooValue = 100;
		m_BarValue = 200;
		m_TestValue = 300;
	}	 

	override 
	int 
	Foo ()
	{
		return printf (
			"CTest.Foo () { %d, %d, %d } \n", 
			m_FooValue,
			m_BarValue,
			m_TestValue
			);
	}

	override 
	int 
	Bar ()
	{
		return printf (
			"CTest.Bar () { %d, %d, %d } \n", 
			m_FooValue,
			m_BarValue,
			m_TestValue
			);
	}
};


int
main ()
{
	printf ("hui govno i muravei\n");

	CTest t = stack new CTest;
	t.Foo ();
	t.Bar ();
	
	IFoo f = t;
	f.Foo ();
	
	IBar b = t;
	b.Bar ();

	return 0;
}

