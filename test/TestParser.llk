LL (3);

global
	: declaration*
	;

declaration
	: type_specifier_modifier+ EToken_Identifier ';'
		{
			printf ("declare %s\n", $2.m_Data.m_String);
		}
	;

type_specifier_modifier
	: EToken_CHAR
	| resolver (type_identifier_rslv) qualified_identifier
	;

qualified_identifier
	: EToken_Identifier ('.' EToken_Identifier)*
	;

type_identifier_rslv
	: qualified_identifier
		{
			printf ("type_qual_identifier_rslv");
			return false;
		}
	;
	
